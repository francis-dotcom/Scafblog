{
  "name": "scafblog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate": "node scripts/feed-to-draft.mjs",
    "generate:auto": "node scripts/feed-to-draft.mjs",
    "generate:pick": "node scripts/feed-to-draft.mjs --interactive",
    "now": "DIRECT_PUBLISH=true NODE_ENV=production node scripts/feed-to-draft.mjs",
    "publish:pr": "node scripts/generate-and-pr.mjs",
    "generate:pr": "npm run generate && npm run publish:pr",
    "preview": "node scripts/feed-to-draft.mjs --preview",
    "generate:debug": "LOG_LEVEL=DEBUG node scripts/feed-to-draft.js",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@giscus/react": "^3.1.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "dotenv": "^17.2.3",
    "inquirer": "^9.2.12",
    "openai": "^6.16.0",
    "p-limit": "^7.2.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-share": "^5.2.2",
    "rss-parser": "^3.13.0",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/types": "3.8.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
